<h1>All Products</h1>

<%= link_to 'New Product' new_product_path %>

<table>
	<tr>
		<th>Title</th>
		<th>Text</th>
	</tr>
	<% @articles.each do |article| %>
		<tr>
			<% @articles.each do |article| %>
			<tr>
				<td><%= article.title %></td>
				<td><%= article.text %></td>
				<td><%= link_to 'Show', article_path(article) %></td>
				<td><%= link_to 'Edit', edit_article_path(article) %></td>
		</tr>
		<% end %>
</table>
